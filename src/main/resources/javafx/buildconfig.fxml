<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox styleClass="root" stylesheets="@theme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.tewan.mcide.app.controllers.ControllerBuildConfigDialog">
   <children>
      <Label text="Build Config">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Separator prefWidth="200.0" />
      <GridPane hgap="10.0" vgap="15.0" VBox.vgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox>
               <children>
                  <HBox>
                     <children>
                        <Label text="World name:" />
                        <Label fx:id="worldname">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
                  <Button maxWidth="1.7976931348623157E308" minWidth="250.0" mnemonicParsing="false" onAction="#browseworld" text="Browse" GridPane.columnIndex="1" />
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets top="18.0" />
         </VBox.margin>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="30.0" left="25.0" right="25.0" top="30.0" />
   </padding>
</VBox>
